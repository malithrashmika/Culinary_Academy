      <ImageView fitHeight="254.0" fitWidth="219.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@asset/icon/Leonardo_Phoenix_A_circular_logo_for_a_culinary_academy_featur_2-removebg-preview.png" />
         </image>
      </ImageView>
      <VBox layoutX="12.0" layoutY="240.0" prefHeight="261.0" prefWidth="195.0">
