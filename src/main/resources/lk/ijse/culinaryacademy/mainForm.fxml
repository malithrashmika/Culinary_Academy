            <JFXButton fx:id="btnDashboard" onAction="#btnDashboardOnAction" prefHeight="50.0" prefWidth="199.0" style="-fx-border-color: white; -fx-background-color: #03001C; -fx-border-width: 3; -fx-border-radius: 5; -fx-background-radius: 10;" text="Dashboard" textFill="WHITE">
               <font>
                  <Font name="DejaVu Sans Condensed Bold" size="17.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="33.0" fitWidth="39.0">
                     <image>
                        <Image url="@asset/icon/visual-data.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
            <JFXButton fx:id="btnStudent" buttonType="RAISED" onAction="#btnStudentOnAction" prefHeight="50.0" prefWidth="199.0" style="-fx-border-color: white; -fx-background-color: #CDC1FF; -fx-border-width: 3; -fx-background-radius: 10; -fx-border-radius: 5;" text="   Student  ">
               <font>
                  <Font name="DejaVu Sans Condensed Bold" size="17.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="37.0" fitWidth="40.0">
                     <image>
                        <Image url="@asset/icon/icons8-student-48.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
            <JFXButton fx:id="btnProgram" onAction="#btnProgramOnAction" prefHeight="50.0" prefWidth="199.0" style="-fx-border-color: white; -fx-background-color: #CDC1FF; -fx-border-width: 3; -fx-background-radius: 10; -fx-border-radius: 5;" text="Academics">
               <font>
                  <Font name="DejaVu Sans Condensed Bold" size="17.0" />
               </font>
