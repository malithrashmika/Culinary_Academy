                     <image>
                        <Image url="@asset/icon/curriculum.png" />
                     </image>
                  </ImageView>
               </graphic></JFXButton>
            <JFXButton fx:id="btnView" onAction="#btnViewOnAction" prefHeight="50.0" prefWidth="199.0" style="-fx-border-color: white; -fx-background-color: #CDC1FF; -fx-border-width: 3; -fx-background-radius: 10; -fx-border-radius: 5;" text="    Reports">
               <font>
                  <Font name="DejaVu Sans Condensed Bold" size="17.0" />
